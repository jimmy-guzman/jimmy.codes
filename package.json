{
  "name": "jimmy.codes",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "dev": "next dev --turbopack",
    "format": "prettier --check --cache .",
