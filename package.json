{
  "name": "jimmy.codes",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "start": "next dev",
    "build": "next build",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --cache --fix",
    "format": "prettier --plugin-search-dir=. --check .",
    "format:fix": "prettier --plugin-search-dir=. --write .",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "prepare": "is-ci || husky install"
  },
  "gitzy": {
    "useCommitlintConfig": true
  },
  "lint-staged": {
    "*.{md,json,yml,js,mjs,ts,tsx,astro}": [
      "prettier -w"
    ],
    "*.{js,mjs,ts,tsx}": [
      "eslint --cache"
    ]
  },
  "dependencies": {
    "@tailwindcss/typography": "0.5.2",
    "clsx": "1.1.1",
    "gray-matter": "4.0.3",
    "highlight.js": "11.5.0",
    "next": "12.1.0",
    "preact": "10.6.6",
    "react": "17.0.2",
    "react-cool-dimensions": "2.0.7",
    "react-dom": "17.0.2",
    "react-markdown": "8.0.0",
    "rehype-autolink-headings": "6.1.1",
    "rehype-external-links": "1.0.1",
    "rehype-highlight": "5.0.2",
    "rehype-slug": "5.0.1",
    "remark-unwrap-images": "3.0.1",
    "tailwindcss": "3.0.23"
  },
  "devDependencies": {
    "@comparto/prettier-config": "1.2.0",
    "@next/eslint-plugin-next": "12.1.0",
    "@testing-library/cypress": "8.0.2",
    "@types/node": "17.0.21",
    "@types/react": "17.0.40",
    "autoprefixer": "10.4.2",
    "commitlint": "16.2.3",
    "cypress": "9.5.1",
    "eslint": "8.11.0",
    "eslint-config-jimmy-guzman": "4.0.0",
    "gitzy": "3.2.0",
    "husky": "7.0.4",
    "is-ci": "3.0.1",
    "jest": "27.5.1",
    "lint-staged": "12.3.5",
    "postcss": "8.4.8",
    "prettier": "2.5.1",
    "prettier-plugin-tailwindcss": "0.1.8",
    "typescript": "4.6.2"
  }
}
