    "commitlint": "17.4.4",
    "cypress": "12.8.1",
    "eslint": "8.36.0",
    "eslint-config-jimmy-guzman": "9.0.1",
    "gitzy": "3.4.0",
    "husky": "8.0.3",
    "is-ci": "3.0.1",
    "jest": "29.5.0",
    "lint-staged": "13.2.0",
    "postcss": "8.4.21",
    "prettier": "2.8.6",
    "prettier-plugin-tailwindcss": "0.2.5",
    "tailwindcss": "3.2.7",
    "typescript": "4.9.5"
