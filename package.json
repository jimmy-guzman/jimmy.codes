    "clsx": "2.1.1",
    "fathom-client": "3.7.2",
    "gray-matter": "4.0.3",
    "highlight.js": "11.11.1",
    "next": "15.1.6",
    "react": "18.3.1",
    "react-cool-dimensions": "3.0.1",
    "react-dom": "18.3.1",
    "react-markdown": "9.0.3",
    "rehype-autolink-headings": "7.1.0",
    "rehype-external-links": "3.0.0",
    "rehype-highlight": "7.0.2",
    "rehype-slug": "6.0.0",
    "rehype-unwrap-images": "1.0.0",
