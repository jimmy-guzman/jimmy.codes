    "@testing-library/cypress": "10.0.3",
    "@types/node": "22.13.1",
    "@types/react": "18.3.18",
    "autoprefixer": "10.4.20",
    "commitlint": "19.7.1",
    "cypress": "14.0.2",
    "daisyui": "4.12.23",
    "eslint": "9.19.0",
    "gitzy": "5.7.1",
    "husky": "9.1.7",
    "is-ci": "4.1.0",
    "jest": "29.7.0",
    "jiti": "2.4.2",
    "lint-staged": "15.4.3",
    "postcss": "8.5.1",
    "prettier": "3.5.0",
    "tailwindcss": "3.4.16",
