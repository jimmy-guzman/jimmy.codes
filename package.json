    "eslint": "8.38.0",
    "eslint-config-jimmy-guzman": "10.1.0",
    "gitzy": "3.4.0",
    "husky": "8.0.3",
    "is-ci": "3.0.1",
    "jest": "29.5.0",
    "lint-staged": "13.2.0",
    "postcss": "8.4.29",
    "prettier": "3.0.3",
    "prettier-plugin-tailwindcss": "0.5.4",
