{
  "name": "jimmy.codes",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "build": "next build",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --cache --fix",
