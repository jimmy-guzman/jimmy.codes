    "*.{js,mjs,ts,tsx}": [
      "eslint --cache"
    ]
  },
  "dependencies": {
    "clsx": "1.2.1",
    "fathom-client": "3.5.0",
    "gray-matter": "4.0.3",
    "highlight.js": "11.7.0",
    "next": "13.2.4",
    "react": "18.2.0",
    "react-cool-dimensions": "3.0.1",
    "react-dom": "18.2.0",
    "react-markdown": "8.0.6",
    "rehype-autolink-headings": "6.1.1",
    "rehype-external-links": "2.0.1",
    "rehype-highlight": "6.0.0",
    "rehype-slug": "5.1.0",
    "remark-unwrap-images": "3.0.1"
